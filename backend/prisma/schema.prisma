generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guild {
  guildId  String   @id
  name     String
  modChannel String?
  voiceCategory String?
  voiceLobby String?  
  nbaChannel String?
  channels   Channel[]
}
model Channel {
  channelId String @id
  name String
  type String
  guildId String
  guild   Guild   @relation(fields: [guildId], references: [guildId])
}